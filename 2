s = input("введите строку: ")
l = len(s)
m = list()
i = 0
while i < l:
    s_int = ''
    a = s[i]
    while '0' <= a <= '9':
        s_int += a
        i += 1
        if i < l:
            a = s[i]
        else:
            break
    i += 1
    if s_int != '':
        m.append(int(s_int))
print(m)
